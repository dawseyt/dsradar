ont:normal normal bold 16px/normal Arial,Helvetica,sans-serif;color:rgba(0,142,198,1);text-align:center;-o-text-overflow:clip;text-overflow:clip;background:rgba(252,252,252,1);-webkit-box-shadow:2px 2px 2px 0 rgba(0,0,0,.2) inset;box-shadow:2px 2px 2px 0 rgba(0,0,0,.2) inset;text-shadow:1px 1px 0 rgba(255,255,255,.66);-webkit-transition:all 200ms cubic-bezier(.42,0,.58,1);-moz-transition:all 200ms cubic-bezier(.42,0,.58,1);-o-transition:all 200ms cubic-bezier(.42,0,.58,1);transition:all 200ms cubic-bezier(.42,0,.58,1)}</style><div id=\\\"radar\\\" style=\\\"background-image: url(\" + bg +\"); height: 100%; width: 100%;\\\"><br><form><table style=\\\"float:right;\\\"><tr><td style=\\\"border:1px;\\\"><a href=\\\"#\\\" class=\\\"btn\\\" onclick=\\\"zoom(1.5)\\\">&nbsp;+&nbsp;</a><a href=\\\"#\\\" class=\\\"btn\\\" onclick=\\\"zoom(0.666)\\\">&nbsp;&ndash;&nbsp;</a><br><br>&nbsp;&nbsp;&nbsp;&nbsp;<input type=\\\"text\\\" class=\\\"enjoy-css\\\" id=\\\"zoomfactor\\\" value=\"+Math.round(rf)+\" onkeydown=\\\"if (event.keyCode == 13){getRadar(this.value); return false;}\\\"><br><br>&nbsp;&nbsp;&nbsp;&nbsp;<input type=\\\"text\\\" class=\\\"enjoy-css\\\" id=\\\"rds\\\" value=\"+Math.round(r)+\"></td></tr><tr><td><INPUT TYPE=\\\"button\\\" onClick=\\\"location.reload();\\\" VALUE=\\\"Refresh\\\"></td></tr></table></div>\"); \r\n\t\t}\r\n\t\tshowCenter(pointh,pointw);\r\n\t\tdocument.getElementById(\"radar\").style.backgroundImage=bg;\r\n\t\tdocument.getElementById(\"rds\").value=Math.round(r);\r\n\t\tdocument.getElementById(\"radar\").style.margin = \"0px 0px 0px 0px\";\r\n\t\tdocument.getElementById(\"radar\").style.border = \"1px solid black\";\r\n\t}\r\n\tfunction showCenter(pointh,pointw)\r\n\t{\r\n\t\tvar div = document.createElement(\"div\");\r\n\t\tdiv.style.background = \"#00ff00\";\r\n\t\tdiv.style.position = \"absolute\";\r\n\t\tdiv.style.top = pointh + \"px\";\r\n\t\tdiv.style.left = pointw + \"px\";\r\n\t\tdiv.style.width = \"4px\";\r\n\t\tdiv.style.height = \"4